# OwnTracks API Configuration
# Copy this file to .env and fill in your values

# Base URL for the OwnTracks Recorder API
# When running locally with Docker, this will be proxied through Vite
VITE_API_URL=/api/0

# Proxy target for development server (to bypass CORS)
# Set this to your OwnTracks server URL
# Example: VITE_PROXY_TARGET=https://owntracks.example.com
VITE_PROXY_TARGET=http://localhost:8083

# Optional: Basic Auth credentials for the OwnTracks Recorder
# Leave empty if authentication is handled by reverse proxy
VITE_API_USERNAME=
VITE_API_PASSWORD=
